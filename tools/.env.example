# WAVE Backend Environment Variables
# Copy this file to .env and fill in your actual values

# API key for experimentee-level access (limited permissions for data collection only)
# This should be distributed to participants and has restricted permissions
EXPERIMENTEE_API_KEY=your_experimentee_api_key_here

# API key for researcher-level access (full permissions for experiment management)
# Keep this secure and never distribute to participants
RESEARCHER_API_KEY=your_researcher_api_key_here

# API key for admin-level access (full permissions including deletion)
# Required ONLY for cleanup operations - keep extremely secure
ADMIN_API_KEY=your_admin_api_key_here

# WAVE Backend URL - MUST match the URL specified in src/js/core/params.js
# Default production URL (verify this matches params.js):
WAVE_BACKEND_URL=https://wave-backend-production-8781.up.railway.app

# For local development, uncomment the line below instead:
# WAVE_BACKEND_URL=http://localhost:8000